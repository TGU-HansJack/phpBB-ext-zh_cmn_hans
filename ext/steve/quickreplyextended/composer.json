{
    "name": "steve/quickreplyextended",
    "type": "phpbb-extension",
    "description": "Adds bbcodes and smilies to quick reply.",
    "homepage": "https://steven-clark.tech/",
    "version": "1.0.1",
    "time": "2025-03-17",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Steve",
            "email": "",
            "homepage": "https://steven-clark.tech/",
            "role": "Author / Developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "composer/installers": "~1.0.0",
        "phpbb/phpbb": ">=3.3.14, <4.0.0@dev"
    },
    "extra": {
        "display-name": "Quick Reply Extended",
        "soft-require": {
            "phpbb/phpbb": ">=3.3.14, <4.0.0@dev"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/quick_reply_extended",
            "filename": "version_check",
            "ssl": true
        }
    }
}